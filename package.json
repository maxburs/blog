    "typescript": "^4.0.3"
  },
  "devDependencies": {
    "@types/jest": "^25.2.2",
    "@types/node": "^14.14.19",
    "@types/react": "^16.9.35",
    "@types/react-dom": "^16.9.8",
    "feed": "^4.2.1",
    "gatsby-remark-prismjs": "^3.10.0",
